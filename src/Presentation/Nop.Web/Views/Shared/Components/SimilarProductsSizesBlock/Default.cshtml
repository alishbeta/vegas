@using Nop.Core.Domain.Common;
@model IEnumerable<SimilarProductSizes>
@if (Model.Count() > 0)
{
    foreach (var size in Model)
    {
        if (size.height == null)
        {
            <a href="@size.productUrl" class="dropdown-item">@size.length см х @size.width см</a>
        }
        else
        {
            <a href="@size.productUrl" class="dropdown-item">@size.width см х @size.length см x @size.height см</a>
        }
    }
}
